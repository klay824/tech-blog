<div>
    <h3>
        Your post history:
    </h3>
    <div>
        {{#each posts as |post|}}
        <div>
            <div>
                <h3>
                    <a href="/post/{{id}}">{{title}}</a>
                </h3>
                <p>{{body}}</p>
            </div>
        </div>
        {{/each}}
    </div>
</div>

<a type="button" class="btn" href='/create'>Create New</a>